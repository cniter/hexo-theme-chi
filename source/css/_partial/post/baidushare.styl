.bdsharebuttonbox {
    min-width: 330px;
    margin: 12px auto;
    text-align: center;

    .fx {
        color: #258fb8;
        padding: 0;
        margin: 6px;
        display: inline-block;
        font: normal normal normal 36px / 1 ForkAwesome;
        background: none;
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
    }

    .fx:hover {
        color: #FFF;
        opacity: 1;
    }
}
